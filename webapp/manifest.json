{
	"_version": "1.12.0",
	"sap.app": {
		"id": "demo_git.demo_git",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"flexEnabled": false,
		"rootView": {
			"viewName": "demo_git.demo_git.view.View1",
			"type": "XML",
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "demo_git.demo_git.i18n.i18n"
				}
			},
			"": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "model/MockData/employee.json"
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "demo_git.demo_git.view",
				"clearTarget": false
			},
			"routes": [{
				"name": "RouteView1",
				"pattern": "",
				"view": "View1",
				"targetControl": "app",
				"targetAggregation": "pages"
			}, {
				"name": "SplitView",
				"pattern": "SplitScreen",
				"view": "sApp",
				"targetControl": "app",
				"targetAggregation": "pages",
				"subroutes": [{
					"name": "Master",
					"pattern": "SplitScreen",
					"view": "master",
					"targetControl": "idSplitApp",
					"targetAggregation": "masterPages",
					"subroutes": [{
						"name": "Detail",
						"pattern": "SplitScreen",
						"view": "detail",
						"targetAggregation": "detailPages",
						"subroutes": [{
							"name": "Detail1",
							"pattern": "SplitScreen/SplitScreen{employeeID}",
							"view": "detail",
							"targetAggregation": "detailPages"
						}]
					}]
				}]
			}]
		}
	}
}